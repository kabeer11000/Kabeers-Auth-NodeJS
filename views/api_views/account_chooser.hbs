<script>
    const code = '{{code}}';
    if (!code) {
        window.location.href = './'
    }

    function login_(u, p) {
        $.ajax({
            url: "/auth/allow",
            method: "POST",
            timeout: 0,
            headers: {
                "Content-Type": "application/x-www-form-urlencoded",
            },
            data: {
                username: u,
                password: atob(p),
                auth_code: code
            },
            tryCount: 0,
            retryLimit: 3,
            success: function (res) {
                if (res) {
                    if (res.callback !== undefined) {
                        window.location.href = res.callback;
                    } else {
                        $('.alert').html('Application Already Authenticated');
                        $('.alert').removeClass('d-none');
                    }
                }
            },
            error: function (xhr, textStatus, errorThrown) {
                if (textStatus === 'timeout') {
                    this.tryCount++;
                    if (this.tryCount <= this.retryLimit) {
                        $.ajax(this);
                        return;
                    }
                    return;
                }
                if (xhr.status === 500) {
                    $('.alert').html('A Server Error Occured');
                    $('.alert').removeClass('d-none');
                } else {
                    $('.alert').html('Some Error Occurred try Clearing all accounts and Re-Login.');
                    $('.alert').removeClass('d-none');
                }
            }
        });
    }

    window.addEventListener('click', () => {
        if (document.querySelector('.alert').classList.contains('d-none')) {
            $('.alert').addClass('d-none');
        }
    });
</script>
<script>
    $(document).ready(function () {

        if (localStorage.getItem("accounts") != null) {
            let data = JSON.parse(localStorage.getItem("accounts"));
            for (let i = 0; i < data.length; i++) {
                $(".main").append(`

                <li class="accountFF px-3">
                    <div>
                        <input name="username" value="${data[i].username}" class="w-0" />
                        <input name="password" value="${data[i].password}" class="w-0" />
                        <button onclick="login_('${data[i].username}', '${data[i].password}')" name="login_user" class="list-group-item px-2">
                        <div class="img-cp position-absolute mt-0"><div class="image-container"><img class="account-image" style="width:2rem;height:2rem" alt="Account Image" src="${data[i].account_image}" />
</div></div>
                            <span class="account-name">${data[i].username}</span>
                            <span class="account-email text-muted">${data[i].email} </span>
                        </button>
                    </div>
                </li>
            `);
            }
        } else {
            window.history.back();
        }
    });
</script>
