            <div class="modal slide" id="exampleModalFullscreen" tabindex="-1"
                 aria-labelledby="exampleModalFullscreenLabel" aria-hidden="true">
                <div class="modal-dialog modal-fullscreen">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title h4" id="exampleModalFullscreenLabel">Full screen modal</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <div class="container">
                                <div class="row">
                                    <ul class="timeline">
                                        <li id="register_app">
                                            <div>
                                                <div class="col-md-6 col-sm-12">
                                                    <div class="mb-3">
                                                        <label for="exampleFormControlInput1" class="form-label">App
                                                            Name</label>
                                                        <input type="name" required class="form-control app_name"
                                                               id="exampleFormControlInput1"
                                                               placeholder="Modern Crane">
                                                    </div>
                                                </div>
                                                <div class="col-md-6 col-sm-12">
                                                    <div class="mb-3">
                                                        <label for="exampleFormControlInput1" class="form-label">Callback
                                                            Domain</label>
                                                        <input type="name" required class="form-control app_callback"
                                                               id="exampleFormControlInput1"
                                                               placeholder="https://example.com/callback">
                                                    </div>
                                                </div>
                                                <div class="col-md-6 col-sm-12">
                                                    <div class="mb-3">
                                                        <label for="exampleFormControlInput1" class="form-label">Privacy
                                                            Policy URL</label>
                                                        <input type="name" required
                                                               class="form-control app_privacy_policy"
                                                               id="exampleFormControlInput1"
                                                               placeholder="https://yourdomain.com/privacy-policy">
                                                    </div>
                                                </div>
                                                <div class="col-md-6 col-sm-12">
                                                    <div class="mb-3">
                                                        <label for="exampleFormControlInput1" class="form-label">TOS
                                                            (Terms Of Use)</label>
                                                        <input type="name" required class="form-control app_tos"
                                                               id="exampleFormControlInput1"
                                                               placeholder="https://yourdomain.com/tos">
                                                    </div>
                                                </div>
                                                <div class="col-md-6 col-sm-12">
                                                    <div class="mb-3 mt-3">
                                                        <div class="form-file form-file-lg-1 mb-3">
                                                            <input type="file"
                                                                   onchange="if(this.files.length) {document.querySelector('.file_name').innerHTML = this.files[0].name}else{document.querySelector('.file_name').innerHTML = 'App Icon'}"
                                                                   class="form-file-input" id="customFileLg" required>
                                                            <label class="form-file-label" for="customFileLg">
                                                                <span class="form-file-text file_name">App Icon</span>
                                                                <span class="form-file-button">Browse</span>
                                                            </label>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6 col-sm-12">
                                                    <div class="mb-3">
                                                        <button class="register_btn btn btn-primary">Register App
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                        </li>
                                        <li id="app_credentials" class="d-none">
